#include <iostream>;
#include <conio.h>

using namespace std;

int main()
{
	int first = 1;
	int second = 1;
	int third;
    int n;


   for ( n = 1; n < 21 ; n++ )
   {
	if (n==0 && n<=1)
		n=0;
	if ( n <=1 )
         third = n;

    else
      {
         third = first + second;
         first = second;
         second = third;
      }
      cout << third << ", ";
   }
